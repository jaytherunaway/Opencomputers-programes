print("Clearing Old Files")
os.execute("del Base")
os.execute("del DC")
os.execute("del CL")
os.sleep(3)
print("getting BaseController")
os.execute("wget https://github.com/jaytherunaway/Opencomputers-programes/raw/main/Base")
os.sleep(3)
os.execute("clear")
print("getting DroneController")
os.execute("wget https://github.com/jaytherunaway/Opencomputers-programes/raw/main/DC")
os.sleep(3)
os.execute("clear")
print("getting Commands List")
os.execute("wget https://github.com/jaytherunaway/Opencomputers-programes/raw/main/CL")

print("Updating...")
os.sleep(5)
print("Update Complete")
os.sleep(2)
os.execute("clear")
